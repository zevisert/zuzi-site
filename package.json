    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.1.1",
    "sinon": "^7.5.0",
    "vinyl-source-stream": "^2.0.0",
    "wct-mocha": "^1.1.0",
    "workbox-cli": "^6.1.1"
  }
}
