    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@polymer/test-fixture": "^4.0.2",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/koa": "^2.13.1",
    "@types/koa-router": "^7.4.1",
    "@types/stripe": "^6.32.13",
    "ansi-grey": "^0.1.1",
    "axe-core": "^3.5.5",
    "chai": "^4.3.3",
    "core-js": "^3.9.1",
    "del": "^3.0.0",
    "del-cli": "^3.0.1",
    "es-dev-server": "^2.1.0",
    "eslint": "^5.16.0",
    "gulp": "^4.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.4",
    "playwright": "^1.9.1",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.15",
