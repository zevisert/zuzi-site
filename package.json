{
  "name": "zuzi-site",
  "version": "1.3.0",
  "contributors": [
    "@zevisert",
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "zevisert/zuzi-site",
  "scripts": {
    "start": "gulp default",
    "build": "polymer build",
    "email:render": "gulp email:render",
    "serve:prpl": "prpl-server --root build/ --config polymer.json",
    "serve:api": "node --experimental-modules server/src/index.mjs",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@material/mwc-icon": "^0.3.4",
    "@polymer/lit-element": "^0.6.4",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "aws-sdk": "^2.395.0",
    "dedent": "^0.7.0",
    "dotenv": "^6.2.0",
    "emailjs": "^2.2.0",
    "koa": "^2.6.2",
    "koa-body": "^4.1.0",
    "koa-mount": "^4.0.0",
    "koa-multer": "^1.0.2",
    "koa-passport": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.0",
    "koa-static": "^5.0.0",
    "mjml": "^4.4.1",
    "mongoose": "^5.4.1",
    "nunjucks": "^3.2.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "prpl-server": "^1.4.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reselect": "^3.0.1",
    "sharp": "^0.22.1",
    "shortid": "^2.2.14",
    "simple-chip": "^1.2.3",
    "stripe": "^6.19.0",
    "tiff.js": "^1.0.0",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.33",
    "@types/stripe": "^6.0.4",
    "ansi-grey": "^0.1.1",
    "axe-core": "^3.0.0",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "eslint": "^5.11.0",
    "gulp": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-nodemon": "^2.4.2",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.8",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.9",
    "polyserve": "^0.27.15",
    "puppeteer": "^1.15.0",
    "sinon": "^7.2.2",
    "vinyl-source-stream": "^2.0.0",
    "wct-mocha": "^1.0.0"
  }
