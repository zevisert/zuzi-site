{
  "name": "zuzi-site",
  "version": "1.4.1",
  "type": "module",
  "contributors": [
    "@zevisert",
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "zevisert/zuzi-site",
  "scripts": {
    "start": "es-dev-server --app-index=index.html --node-resolve --watch",
    "build": "del-cli build && rollup -c rollup.config.js",
    "email:render": "gulp email:render",
    "serve:prpl": "prpl-server --root build/ --config prpl.config.json",
    "serve:api": "node --experimental-modules server/src/index.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@koa/multer": "^3.0.0",
    "@material/mwc-icon": "^0.20.0",
    "@sentry/browser": "^6.2.1",
    "@sentry/tracing": "^6.2.1",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "@zevisert/prpl-server": "^1.4.3",
    "aws-sdk": "^2.858.0",
    "dedent": "^0.7.0",
    "dotenv": "^8.2.0",
    "emailjs": "^3.4.0",
    "justified": "^1.0.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-mount": "^4.0.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^10.0.0",
    "koa-session": "^6.1.0",
    "koa-static": "^5.0.0",
    "lit": "^2.0.0",
    "mjml": "^4.9.0",
    "mongoose": "^5.11.19",
    "nunjucks": "^3.2.3",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sharp": "^0.27.2",
    "shortid": "^2.2.16",
    "simple-chip": "^1.2.3",
    "stripe": "^8.138.0",
    "tiff.js": "^1.0.0",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@polymer/test-fixture": "^4.0.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/koa": "^2.13.1",
    "@types/koa-router": "^7.4.1",
    "ansi-grey": "^0.1.1",
    "axe-core": "^4.1.3",
    "chai": "^4.3.3",
    "core-js": "^3.9.1",
    "del": "^6.0.0",
    "del-cli": "^3.0.1",
    "es-dev-server": "^2.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "gulp": "^4.0.2",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.88.2",
    "rollup": "^2.40.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-minify-html-literals": "^1.2.5",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.1.1",
    "sinon": "^9.2.4",
    "vinyl-source-stream": "^2.0.0",
    "wct-mocha": "^1.1.0",
    "workbox-cli": "^6.1.1"
  }
}
